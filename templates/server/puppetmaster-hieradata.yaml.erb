# Secrets - default file generated by Puppet, safe to overwrite
---
hello_worlds:
  - "hello from private/production/hieradata/nodes/puppet.bitswarm.internal.yaml (generated by <%= @caller_module_nam %>)"

default_admin_acct_name: '<%= scope.lookupvar('bsl_puppet::config::default_admin_acct_name') %>'
default_admin_acct_pass: '<%= scope.lookupvar('bsl_puppet::config::default_admin_acct_pass') %>'

<%- if scope.lookupvar('bsl_puppet::config::r10k_github_api_token') -%>
github_api_token: '<%= scope.lookupvar('bsl_puppet::config::r10k_github_api_token') %>'
<%- end -%>

<%- if scope.lookupvar('bsl_puppet::config::r10k_webhook_user') != scope.lookupvar('bsl_puppet::config::default_admin_acct_user') -%>
bsl_puppet::config::r10k_webhook_user: '<%= scope.lookupvar('bsl_puppet::config::r10k_webhook_user') %>'
<%- end -%>
<%- if scope.lookupvar('bsl_puppet::config::r10k_webhook_pass') != scope.lookupvar('bsl_puppet::config::default_admin_acct_pass') -%>
bsl_puppet::config::r10k_webhook_pass: '<%= scope.lookupvar('bsl_puppet::config::r10k_webhook_pass') %>'
<%- end -%>

<%- if scope.lookupvar('bsl_puppet::config::puppetboard_user') != scope.lookupvar('bsl_puppet::config::default_admin_acct_user') -%>
bsl_puppet::config::r10k_webhook_user: '<%= scope.lookupvar('bsl_puppet::config::puppetboard_user') %>'
<%- end -%>
<%- if scope.lookupvar('bsl_puppet::config::puppetboard_pass') != scope.lookupvar('bsl_puppet::config::default_admin_acct_pass') -%>
bsl_puppet::config::r10k_webhook_pass: '<%= scope.lookupvar('bsl_puppet::config::puppetboard_pass') %>'
<%- end -%>

<%- if scope.lookupvar('bsl_puppet::config::server_aws_api_key') -%>
bsl_puppet::config::server_aws_api_key: '<%= scope.lookupvar('bsl_puppet::config::server_aws_api_key') %>'
bsl_puppet::config::server_aws_api_secret: '<%= scope.lookupvar('bsl_puppet::config::server_aws_api_secret') %>'
<%- end -%>
