<% unless @rsources.empty? -%>
    <% @source_keys.sort.each do |source| -%>
        <%=source-%>:
        <% @r10k_sources[source].sort.each do |key,value| -%>
            <%=key-%>: <%=value%>
        <% end -%>
    <% end -%>
<% end %>
<% if !@git_settings.empty? -%>
    git:
    <% @git_settings.sort.each do |key, value| -%>
        <%=key-%>: <%=value%>
    <% end -%>
<% end -%>
<% if !@forge_settings.empty? -%>
    forge:
    <% @forge_settings.sort.each do |key, value| -%>
        <%=key-%>: <%=value%>
    <% end -%>
<% end -%>
